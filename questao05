#include <stdio.h>
#include <stdlib.h>

int main(){

    double nota1, nota2, nota3, media;

    printf ("Digite nota 1: ");
    scanf ("%lf", &nota1);

    printf ("Digite nota 2: ");
    scanf ("%lf", &nota2);

    printf ("Digite nota 3: ");
    scanf ("%lf", &nota3);


    media = ((nota1 * 2 + nota2 * 3 + nota3 * 5) / (2 + 3 + 5));

    printf ("A media ponderada e: %.1lf", media);

return 0;
}
