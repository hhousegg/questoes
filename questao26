#include <stdio.h>
#include <stdlib.h>

int main(){



		int var1, calc;

		printf ("Digite um valor: ");
		scanf ("%d", &var1);

		printf ("%d\n", var1);
		printf ("%d nota(s) de R$ 100,00\n", var1/100);
		calc = (var1%100);


		printf ("%d nota(s) de R$ 50,00\n", calc/50);
		calc = (calc%50);


		printf ("%d nota(s) de R$ 20,00\n", calc/20);
		calc = (calc%20);


		printf ("%d nota(s) de R$ 10,00\n", calc/10);
		calc = (calc%10);


		printf ("%d nota(s) de R$ 5,00\n", calc/5);
		calc = (calc%5);


		printf ("%d nota(s) de R$ 2,00\n", calc/2);
		calc = (calc%2);

		printf ("%d nota(s) de R$ 1,00\n", calc/1);


return 0;
}
