#include <stdio.h>
#include <stdlib.h>

int main(){

    int a, b, c, d, inteiro;

    printf ("Digite quatro valores: ");
    scanf ("%d %d %d %d", &inteiro, &a, &b, &c);

    if  (inteiro == 1){
        if (a > b && a > c && b >c){
            printf ("%d %d %d\n", c, b, a);
        }
        else if (a > b && a > c && c > b){
            printf ("%d %d %d\n", b, c, a);
        }
        else if (b > a && b > c && a > c){
            printf ("%d %d %d\n", c, a, b);
        }
        else if (b > a && b > c && c > a){
            printf ("%d %d %d\n", a, c, b);
        }
        else if (c > a && c > b && b > a){
            printf ("%d %d %d\n", a, b, c);
        }
        else if (c > a && c > b && a > b){
            printf ("%d %d %d\n", b, a, c);
        }
    }
    if  (inteiro == 2){
        if (a < b && a < c && b < c){
            printf ("%d %d %d\n", c, b, a);
        }
        else if (a < b && a < c && c < b){
            printf ("%d %d %d\n", b, c, a);
        }
        else if (b < a && b < c && a < c){
            printf ("%d %d %d\n", c, a, b);
        }
        else if (b < a && b < c && c < a){
            printf ("%d %d %d\n", a, c, b);
        }
        else if (c < a && c < b && b < a){
            printf ("%d %d %d\n", a, b, c);
        }
        else if (c < a && c < b && a < b){
            printf ("%d %d %d\n", b, a, c);
        }
    }
    if  (inteiro == 3){
        if (a < b && a < c && b < c){
            printf ("%d %d %d\n", b, c, a);
        }
        else if (a < b && a < c && c < b){
            printf ("%d %d %d\n", c, b, a);
        }
        else if (b < a && b < c && a < c){
            printf ("%d %d %d\n", a, c, b);
        }
        else if (b < a && b < c && c < a){
            printf ("%d %d %d\n", c, a, b);
        }
        else if (c < a && c < b && b < a){
            printf ("%d %d %d\n", b, a, c);
        }
        else if (c < a && c < b && a < b){
            printf ("%d %d %d\n", a, b, c);
        }
    }

    return 0;
}
